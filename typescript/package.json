{
  "name": "fancy-todo",
  "version": "1.0.0",
  "description": "fancy todo with typescript",
  "main": "index.js",
  "scripts": {
    "db:up": "docker compose up -d && dbmate up",
    "db:down": "docker compose down",
    "db:up:pg": "docker compose up -d pg && dbmate up",
    "db:up:es": "docker compose up -d es && dbmate up",
    "build": "tsc && ncc build dist/index.js -o ncc",
    "dev": "npm run build:ts && node ncc/index.js",
    "prestart": "docker build . --tag ts",
    "start": "docker compose up -d"
  },
  "keywords": [],
  "author": "uulwake",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.10.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.8.10",
    "@vercel/ncc": "^0.38.1",
    "typescript": "^5.2.2"
  }
}
